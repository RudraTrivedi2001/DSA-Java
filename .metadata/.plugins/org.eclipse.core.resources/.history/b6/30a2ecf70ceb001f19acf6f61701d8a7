package com.Arrays.java.in;

public class PrintSubArray {
	public static void main(String[] args) {
		int arr[] = {0,1,2,3};
		
		
		for (int i = 0; i < arr.length; i++) {
			
			int start = i;
			for (int j = start; j < arr.length; j++) {
				
			  for (int k = start; k <= i; k++) {
				System.out.println(arr[k]);
			}
			  System.out.println();
		}
		System.out.println();
	}
		
		
		
		
		
	}

}
