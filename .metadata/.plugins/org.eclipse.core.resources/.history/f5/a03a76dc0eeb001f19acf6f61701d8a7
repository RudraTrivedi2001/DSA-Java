package com.Arrays.java.in;

public class PrintSubArray {
	public static void main(String[] args) {
		int arr[] = {0,1,2,3};
		
		
		for (int i = 0; i < arr.length; i++) {
			
			

			for (int k = i; k < arr.length; k++) {
					
			
			for (int j = i; j <=k; j++) {
				System.out.print(arr[j] + " ");
				
				
				
			
		}
			System.out.println("__________");
		
	  }
			System.out.println("__________");
		
   }
		
		
		
	}

}
