package com.Arrays.java.in;

public class PrintSubArray {
	public static void main(String[] args) {
		int arr[] = {0,1,2,3};
		
		
		for (int i = 0; i < arr.length; i++) {
			
			int start = i;
			int end = i+1;
			for (int j = 0; j <=i; j++) {
				System.out.print(arr[j]);
			
		}
		System.out.println(start);
	}
		
		
		
		
		
	}

}
