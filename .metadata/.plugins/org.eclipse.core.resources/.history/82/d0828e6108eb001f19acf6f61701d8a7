package com.Arrays.java.in;



public class PrintSubArray 
{
	
	public static void subArray(int arr[]) 
	{
		
//		
//		int initial = arr[i];
//		int end = arr[i+1];
		
		for (int i = 0; i < arr.length; i++)
		{
		for (int j = 0; j <= i; j++) {
			
			for (int k = i + 1; k < arr.length; k++)
			{
				System.out.print(arr[k]);
			}
			
			System.out.println();
			
		}	
		System.out.println();
			
		}
		
	}

	
	public static void main(String[] args)
	{
		int arr[] = {2,4,6,8,10};
		subArray(arr);
	}
	
}
