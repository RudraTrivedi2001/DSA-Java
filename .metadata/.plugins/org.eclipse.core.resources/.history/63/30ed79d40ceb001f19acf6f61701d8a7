package com.Arrays.java.in;

public class PrintSubArray {
	public static void main(String[] args) {
		int arr[] = {0,1,2,3};
		
		
		for (int i = 0; i < arr.length; i++) {
			
			int start = i;
			for (int j = start; j < arr.length; j++) {
				
				System.out.print(arr[j]);
			}
			System.out.println();
		}
		
		
		
		
		
	}

}
